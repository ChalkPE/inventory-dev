    #container
      #content
        // subcon
        .show-listing-wrapper
          .listing-metadata-wrapper
            .metadata
              .metadata-item
                p
                  span.sub-title.bold
                    | #{timeAgo.editDate}
                  span.time
                    | #{timeAgo.uploadDate}
            h3.social-share-widget
              .social-share-widget-item.twitter
                a(href='#', target='_blank')
                  img(src='/images/2.click_page_3.png', alt='Twittericon')
              .social-share-widget-item.facebook
                a(href='', target='_blank')
                  img(src='/images/2.click_page_4.png', alt='Facebookicon')
          .show-listing
            .listing-photos-wrapper
              ul.listing-photos
                li.listing-photo-item
                  img(src=".." + data.productImage.cover)
                each val in data.productImage.subpics
                  li.listing-photo-item
                    img(src=".." + val)
            .listing-details-wrapper.topline
              h1.designer.jumbo
                a(href='#') #{data.productSubTitle} 
              h2.listing-title.sub-title
                | #{data.productTitle}
              h2.listing-size.sub-title
                | #{data.productSize}
              .listing-price
                p
                  span(style="font-size: 14px;")
                    img(src='/images/2.click_page_1.png')
                  span.ptxt(style="font-size: 14px;") #{data.productPrice}
                p
                  span.goodtog(style="font-size: 14px;")
                    img.goodico(src='/images/2.click_page_2.png', alt='GOOD')
                    img(style='display:none;', src='/images/2.click_page_2on.png')
                  span.ptxt(style="font-size: 14px;") #{data.like}
                p.price-comparison
                  a(href='#') price comparison
              .pricebtnw
                .pricebtn#paypal buy it now
                .pricebtn.mno.offer Make an offer
                p.ask-a-question
                  a.question(href='#') Ask a question?
              .listing-description
                h3.section-header
                  | description
                p
                  | #{data.description}
              .listing-shipping
                h3.section-header
                  | shipping
                .shipping
                  each val, key in data.shipping
                    .item
                      .location #{key}
                      .dots
                      .amount #{val}
              .listing-seller-profile
                h3.section-header
                  | seller
                .user-widget.medium
                  h1.sub-title.semi-bold
                    a(href='/u/' + data.seller) #{data.seller}
                  .feedback
                    .feedbackwrap
                      ul
                        i.fa.fa-star-o
                        i.fa.fa-star-o
                        i.fa.fa-star-o
                        i.fa.fa-star-o
                        i.fa.fa-star-o
                      p Feedback
                  .user-widget-bottom
                    p.user-location
                      | United States
                    p.dimensions
                      | No measurements entered.
                  .bought-and-soldwrap
                    .bought-and-sold
                      h2.sub-title.seller-item-title
                        a.sales-item(href="#")
                          span #{itemCount} Items for Sale
                    .seller-score-wrapper
                      p.seller-score-top
                        a.green(href='/inventory/mypage/feedback.html') 5/5 Feedback
              .listing-related
                .comments-top
                  h3.section-header.sub-title
                    | related items
                .productw
                  each product in moreItems
                    .product
                      a(href='#')
                        .listings-thumbnail
                          img(src=product.productImage.cover, alt='Thumbnail')
                        .information
                          ol
                            li
                              span.product_name #{product.productSubTitle}
                            li
                              span.short_desc #{product.description}
                            li
                              span.product_size #{product.productSize}
                            li.price_row
                              .price_row1
                                .product_good
                                  span.goodtog
                                    img.goodico(src='/images/product_good_ico.png', alt='GOOD')
                                    img(style='display:none;', src='/images/product_good_ico2.png')
                                  span.count #{product.like}
                                .product_price
                                  img(src='/images/product_dollar_ico.png', alt='DOLLAR')
                                  span.count #{product.productPrice}
                              .price_row2
                                span.write_date #{product.timeAgo}
                .view-more View More
              .listing-comments
                .comments-top
                  h3.section-header
                    | comments
                each comment in comments
                  .comment-box
                      .comment-profile_pic
                          img(src=comment.authorProfilePic)
                      .comment-profile_name
                          span #{comment.author}
                      .comment-text
                          span #{comment.description}
                p.no-comments
                  | There are no comments yet
                p.comment-this-item
                  input(type="text", placeholder="leave comment")
                button.submit-this-comment Submit
