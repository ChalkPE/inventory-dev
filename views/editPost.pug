    script.
      let shippingList = !{JSON.stringify(shipping).replace(/<\//g, '<\\/')}
      let images = !{JSON.stringify(productImage).replace(/<\//g, '<\\/')}

      for(let country in shippingList) {
        let countryElem = $(`span:contains(${country})`).parents('dl').find('input')
        countryElem[0].checked = true
        countryElem[1].value = shippingList[country]
      }

      $('##{market}, ##{gender}').find('.fa').css("color", "rgb(56, 61, 65)")
      $('select #'+ "#{productCategory}".toLowerCase())[0].selected = 'selected'
      $('input[name="item_name"]')[0].value = "#{productTitle}"
      $('input[name="designer_name"]')[0].value = "#{productSubTitle}"
      $('.description')[0].value = "#{description}"
      $('input[name="price"]')[0].value = "#{productPrice}"
      $('input[name="paypal_account"]')[0].value = "#{paypalAccount}"

      if(#{buyItNow}) $('input[name="buy_it_now"]').prop('checked', true)
      if(#{acceptOffers}) $('input[name="accept_offes"').prop('checked', true)

      imageObject.cover = images.cover
      for(let count in images.subpics) {
        let picURL = images.subpics[count]
        imageObject.subpics.push(images.subpics[count])

        $('.shipping_box').find('.image_box').eq(count).css({
          "background-image": `url('${picURL}')`,
          "background-size": "cover"
        })
      }

